<h2>User info</h2>
  <div class="container">
    <div class="row">
      <div class="col-md-3">
        <%= render partial: 'users/info', locals: { user: current_user} %>
        <h2 class="mt-3">New book</h2>
        <%= render partial: 'books/form', locals: { book: @book } %>
      </div>
      <div class="col-md-8 offset-md-1">
        <h2>Users</h2>
        <%= render partial: 'users/index', locals: { users: @users} %>
      </div>
    </div>
  </div>